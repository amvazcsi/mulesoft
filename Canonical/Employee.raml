#%RAML 1.0 DataType
type: object
properties:
  id: 
    type: number
    required: false
  fistname: 
    type: string
    required: false
  lastname: 
    type: string
    required: false
  gender: 
    type: string
    required: false
  hire_date: 
    type: date-only
    required: false
  bank: 
    !include Bank.raml
    required: false
  role: 
    type: array 
    items: !include Role.raml
    required: false